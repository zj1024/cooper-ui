{
  "name": "cooper-ui",
  "version": "0.0.1",
  "description": "A React UI library",
  "main": "./dist/lib/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "yarn docs:env",
    "docs:env": "webpack-dev-server --config ./config/webpack.config.dev",
    "docs:build": "webpack --progress --config ./config/webpack.config.dev",
    "build": "webpack --progress --config ./config/webpack.config.prod",
    "build:ts": "tsc libs/*.* --outDir ./cooper-ui --declaration --jsx react",
    "build:scss": "gulp -f ./build/shadking-css.gulpfile.js",
    "publish:pre": "rm -rf ./cooper-ui && npm run build:ts && npm run build:scss && cp -R  ./libs/assets ./cooper-ui/",
    "test": "jest",
    "ci": "JEST_JUNIT_OUTPUT_DIR=./test-results/jest/ JEST_JUNIT_OUTPUT_NAME=results.xml jest --ci --reporters=jest-junit",
    "build:copy": "babel libs --out-dir ./cooper-ui --copy-files",
    "publish:local": "rm -rf ./dist && npm run build:copy && cp -R libs/assets/ ./cooper-ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zj1024/cooper-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zj1024/cooper-ui/issues"
  },
  "homepage": "https://github.com/zj1024/cooper-ui#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e",
      "pre-push": "npm test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js}": [
      "npx eslint --fix"
    ],
    "*.{ts,tsx,js,less,json,md}": [
      "npx prettier --write"
    ],
    "*.{ts,tsx}": [
      "npx tslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@hot-loader/react-dom": "^16.11.0+4.12.16",
    "@loadable/component": "^5.10.3",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.21",
    "@types/marked": "^0.7.2",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-syntax-highlighter": "^11.0.2",
    "@types/react-test-renderer": "^16.9.1",
    "@types/react-transition-group": "^4.2.3",
    "assembly-css": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-standalone": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitlint": "^8.2.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.16.0",
    "fibers": "^4.0.2",
    "file-loader": "^4.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^10.0.0-1",
    "marked": "^0.8.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.19.0",
    "raw-loader": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.19",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^11.0.2",
    "react-test-renderer": "^16.11.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "thread-loader": "^2.1.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "typescript": "^3.6.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
